{
  "name": "obsidian-spaced-repetition-scheduler",
  "version": "0.1.1",
  "description": "Spaced repetition scheduler for Obsidian with SM-2 algorithm and AI quiz generation",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "typecheck": "tsc -noEmit -skipLibCheck"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "spaced-repetition",
    "srs",
    "sm2",
    "flashcards",
    "learning",
    "ai",
    "quiz"
  ],
  "author": "eohjun",
  "license": "MIT",
  "devDependencies": {
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.27.2",
    "obsidian": "^1.11.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
